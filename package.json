{
	"name": "storybook-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test:unit": "vitest",
		"test": "pnpm run test:e2e && pnpm run test:unit",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"deploy": "vite build && wrangler deploy",
		"deploy:preview": "vite build && wrangler deploy --dry-run",
		"cf-typegen": "wrangler types ./src/types/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.3",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.56.1",
		"@storybook/addon-a11y": "^10.0.8",
		"@storybook/addon-docs": "^10.0.8",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.0.8",
		"@storybook/sveltekit": "^10.0.8",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"@vitest/browser-playwright": "^4.0.14",
		"eslint": "^9.39.1",
		"eslint-plugin-storybook": "^10.0.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"storybook": "^10.0.8",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.14",
		"wrangler": "^4.51.0"
	},
	"dependencies": {
		"@fontsource/commissioner": "^5.2.10",
		"@fontsource/roboto": "^5.2.8",
		"@fontsource/roboto-mono": "^5.2.8",
		"@fontsource/roboto-slab": "^5.2.8",
		"browser-fs-access": "^0.38.0",
		"marked": "^17.0.1"
	}
}
